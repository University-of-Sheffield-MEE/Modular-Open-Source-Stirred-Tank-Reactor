        {
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb"
          },
          "query": "from(bucket: \"homeassistant\")\r\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\r\n  |> filter(fn: (r) => r[\"domain\"] == \"sensor\")",
          "refId": "A"
        }
      ],
      "title": "All Data",
      "transformations": [
        {
          "id": "renameByRegex",
          "options": {
            "regex": "value .+",
            "renamePattern": "Value"
          }
        }
      ],
      "type": "table"
    }
